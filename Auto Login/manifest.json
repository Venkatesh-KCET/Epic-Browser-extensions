{
   "author": "@Ching",
   "background": {
      "persistent": true,
      "scripts": [ "assets/js/background.js" ]
   },
   "browser_action": {
      "default_icon": "assets/img/logo.png",
      "default_popup": ""
   },
   "content_scripts": [ {
      "js": [ "assets/js/jquery-2.0.1.min.js", "assets/js/content.js" ],
      // "matches": [ "*://search.epicbrowser.com/login/" ],
      "matches": [ "*://epicsearch.in/login/" ],
      "run_at": "document_start"
   } ],
   "description": "Auto Login extension for epic",
   "icons": {
      "128": "assets/img/logo_128x128.png",
      "16": "assets/img/logo_16x16.png",
      "48": "assets/img/logo_48x48.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "09_Auto Login",
   "permissions": [ "unlimitedStorage", "management", "tabs", "webRequest", "cookies", "*://search.epicbrowser.com/*", "*://epicsearch.in/*", "*://*.google.com/", "webRequestBlocking", "storage", "activeTab", "identity", "fileSystem" ],
   "update_url": "http://updates.epicbrowser.com/extensions/updates.xml",
   "version": "1.4"
}
